<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!-- Exclude serialVersionUID warnings for Serializable classes -->
    <Match>
        <Bug pattern="SE_NO_SERIALVERSIONID"/>
    </Match>
    
    <!-- Exclude JavaFX View classes from certain checks -->
    <Match>
        <Class name="~.*View"/>
        <Bug pattern="UWF_FIELD_NOT_INITIALIZED_IN_CONSTRUCTOR"/>
    </Match>
    
    <!-- Add more exclusions as needed during testing -->
</FindBugsFilter>
